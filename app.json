{
  "expo": {
    "name": "Loote Lite",
    "slug": "looteverything",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#2874f0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "package": "com.sktech.lootelite"
    },
    "packagerOpts": {
      "nonPersistent": "--nonPersistent"
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
